    "check:devmode": "node test/check-devmode.mjs",
    "lint": "eclint check",
    "lint:fix": "eclint fix"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.47.1",
