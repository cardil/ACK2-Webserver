    "@types/socket.io-client": "^1.4.36",
    "eclint": "^2.8.1",
    "node-fetch": "^3.3.2",
    "patch-package": "^8.0.1",
    "socket.io": "^4.8.1",
    "svelte": "^5.41.0",
    "svelte-check": "^4.3.3",
    "typescript": "^5.9.3",
    "vite": "^7.1.10"
  },
  "dependencies": {
    "@types/plotly.js-dist-min": "^2.3.4",
    "plotly.js-dist-min": "^3.2.0",
    "socket.io-client": "^4.8.1"
  }
